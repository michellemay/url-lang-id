{
  "mappings":[
    {
      "name":"mytest",
      "extend":["ISO-639-ALPHA-2", "ISO-639-ALPHA-3"],
      "add":{"en":"english,ANGLAIS","es":"spanish,espagnol"},
      "override":{"fr":"french,fran√ßais"},
      "filter":"en,fr,de,es,it",
      "casesensitive":"true"
    },
    {
      "name": "mytest2",
      "extend":["ISO-639-ALPHA-2"],
      "casesensitive":"true"
    }
  ],

  "matchers":[
    {
      "name":"mypathregex1",
      "urlpart":"path",
      "patterns":["/docs/(?<lang>[^/]+)/index\\.html"],
      "mapping":"mytest"
    },
    {
      "name":"mypathregex2",
      "urlpart":"path",
      "patterns":["/KB/(?<lang>[^/]+)/index\\.html"],
      "mapping":"mytest",
      "casesensitive":"true"
    },
    {
      "name":"myquerystring1",
      "urlpart":"querystring",
      "patterns":["(cv_)?lang(uage)?=(?<lang>.*)","loc=(?<lang>.*)"],
      "casesensitive":"true"
    },
    {
      "name":"myquerystring2",
      "urlpart":"querystring",
      "patterns":["lr=(?<lang>.*)"]
    }
  ],

  "profiles": [
    {
      "name":"custom",
      "domains":["mystuff.stuff","my.*\\.stuff"],
      "mapping":"mytest",
      "matchers":[
        {
          "matcher":"mypathregex1"
        },
        {
          "matcher":"mypathregex2"
        },
        {
          "matcher":"myquerystring1",
          "mapping":"mytest2"
        },
        {
          "matcher":"myquerystring2"
        }
      ]
    },
    {
      "name":"other",
      "domains":["other\\.com"],
      "mapping":"mytest2",
      "matchers":[
        {
          "matcher":"myquerystring2"
        }
      ]
    }
  ]
}

